<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ebiz.dto.food.FoodPriceLevel" table="tblFoodPriceLevel">
        <id name="foodPriceLevelId" type="java.lang.String">
            <column name="foodPriceLevelId" />
            <generator class="assigned" />
        </id>
        <property name="startPrice" type="long">
            <column name="startPrice" />
        </property>
        <property name="endPrice" type="long">
            <column name="endPrice" />
        </property>
    </class>
</hibernate-mapping>
