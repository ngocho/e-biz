<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ebiz.dto.food.Food" table="tblFood">
        <id name="foodId" type="java.lang.Long">
            <column name="foodId" />
            <generator class="assigned" />
        </id>
        <property name="foodName" type="java.lang.String">
            <column name="foodName" />
        </property>
        <property name="foodPriceLevelId" type="java.lang.String">
            <column name="foodPriceLevelId" />
        </property>
        <property name="productAttributeId" type="java.lang.String">
            <column name="productAttributeId" />
        </property>
        <property name="foodStatusId" type="java.lang.String">
            <column name="foodStatusId" />
        </property>
        <property name="foodTypeId" type="java.lang.String">
            <column name="foodTypeId" />
        </property>
        <property name="startDate" type="java.util.Date">
            <column name="startDate" />
        </property>
        <property name="expDate" type="java.util.Date">
            <column name="expDate" />
        </property>
        <property name="number" type="int">
            <column name="number" />
        </property>
        <property name="numberOrder" type="int">
            <column name="numberOrder" />
        </property>
        <property name="detail" type="java.lang.String">
            <column name="detail" />
        </property>
        <property name="cooking" type="java.lang.String">
            <column name="cooking" />
        </property>
        <property name="calo" type="java.lang.String">
            <column name="calo" />
        </property>
        <property name="price" type="long">
            <column name="price" />
        </property>
        <property name="pricePromotion" type="long">
            <column name="pricePromotion" />
        </property>
        <property name="providerID" type="java.lang.String">
            <column name="providerID" />
        </property>
        <property name="urlKey" type="java.lang.String">
            <column name="urlKey" />
        </property>
        <property name="saleProduct" type="int">
            <column name="saleProduct" />
        </property>
        <property name="uploadDate" type="java.util.Date">
            <column name="uploadDate" />
        </property>
        <property name="isDisplay" type="int">
            <column name="isDisplay" />
        </property>
    </class>
</hibernate-mapping>
