<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ebiz.dto.account.customer.Customer" table="tblCustomer">
        <id name="customerId" type="java.lang.String">
            <column name="customerId" />
            <generator class="assigned" />
        </id>
        <property name="customerPassword" type="java.lang.String">
            <column name="customerPassword" />
        </property>
        <property name="customerName" type="java.lang.String">
            <column name="customerName" />
        </property>
        <property name="customerGender" type="java.lang.String">
            <column name="customerGender" />
        </property>
        <property name="customerBirth" type="java.util.Date">
            <column name="customerBirth" />
        </property>
        <property name="customerEmail" type="java.lang.String">
            <column name="customerEmail" />
        </property>
        <property name="customerPhone" type="java.lang.String">
            <column name="customerPhone" />
        </property>
        <property name="customerType" type="java.lang.String">
            <column name="customerType" />
        </property>
        <property name="customerSumMoney" type="long">
            <column name="customerSumMoney" />
        </property>
        <property name="customerMark" type="int">
            <column name="customerMark" />
        </property>
        <property name="isPay" type="boolean">
            <column name="isPay" />
        </property>
        <property name="payOnlineType" type="java.lang.String">
            <column name="payOnlineType" />
        </property>
        <property name="isAdEmail" type="boolean">
            <column name="isAdEmail" />
        </property>
        <property name="isAdPhone" type="boolean">
            <column name="isAdPhone" />
        </property>
        <property name="isActive" type="boolean">
            <column name="isActive" />
        </property>
        <property name="xuOnline" type="long">
            <column name="xuOnline" />
        </property>
        <property name="loginDate" type="java.util.Date">
            <column name="loginDate" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="url" />
        </property>
        <property name="buildingName" type="java.lang.String">
            <column name="buildingName" />
        </property>
        <property name="districtName" type="java.lang.String">
            <column name="districtName" />
        </property>
        <property name="homeNumber" type="java.lang.String">
            <column name="homeNumber" />
        </property>
        <property name="streetName" type="java.lang.String">
            <column name="streetName" />
        </property>
        <property name="wardName" type="java.lang.String">
            <column name="wardName" />
        </property>
    </class>
</hibernate-mapping>
