<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ebiz.dto.account.provider.Provider" table="tblProvider">
        <id name="providerId" type="java.lang.String">
            <column name="providerId" />
            <generator class="assigned" />
        </id>
        <property name="providerPassword" type="java.lang.String">
            <column name="providerPassword" />
        </property>
        <property name="providerName" type="java.lang.String">
            <column name="providerName" />
        </property>
        <property name="providerGender" type="java.lang.String">
            <column name="providerGender" />
        </property>
        <property name="providerBirth" type="java.util.Date">
            <column name="providerBirth" />
        </property>
        <property name="providerEmail" type="java.lang.String">
            <column name="providerEmail" />
        </property>
        <property name="providerPhone" type="java.lang.String">
            <column name="providerPhone" />
        </property>
        <property name="providerAddress" type="java.lang.String">
            <column name="providerAddress" />
        </property>
        <property name="providerAccountPayment" type="java.lang.String">
            <column name="providerAccountPayment" />
        </property>
        <property name="xuOnline" type="long">
            <column name="xuOnline" />
        </property>
        <property name="authentication" type="java.lang.String">
            <column name="authentication" />
        </property>
        <property name="loginDate" type="java.util.Date">
            <column name="loginDate" />
        </property>
        <property name="dateRegister" type="java.util.Date">
            <column name="dateRegister" />
        </property>
        <property name="detail" type="java.lang.String">
            <column name="detail" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="url" />
        </property>
        <property name="isActive" type="boolean" access="field">
            <column name="isActive" />
        </property>
        <property name="activeCode" type="java.lang.String">
            <column name="activeCode" />
        </property>
    </class>
</hibernate-mapping>
