<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
  <form-beans>
    <form-bean name="providerForm" type="ebiz.form.ProviderForm"/>
    <form-bean name="providerFormLogin" type="ebiz.form.ProviderForm"/>
    <form-bean name="foodForm" type="ebiz.form.FoodForm"/>
  </form-beans>
  <action-mappings>
    <action path="/registerProvider" type="ebiz.action.account.provider.ProviderRegisterLink">
      <forward name="success" redirect="true" path="home_provider_register"/>
    </action>
    <action path="/loginProvider" forward="home_provider_login">
    </action>
    <action path="/transferUpImage" forward="home_provider_upload">
    </action>
    <action path="/transferUpImage1" forward="home_provider_register">
    </action>
    <action path="/uploadLink" type="ebiz.action.account.provider.UploadLink">
      <forward name="success" redirect="true" path="home_provider_upload"/>
    </action>
    <action path="/provider" type="ebiz.action.account.provider.AuthenticationProvider">
      <forward name="success" redirect="true" path="/displayProduct.vn"/>
      <forward name="failure" redirect="true" path="home_provider_login"/>
    </action>
    <action path="/loginProviderAcc" name="providerFormLogin" type="ebiz.action.account.provider.Login" scope="session" validate="true" input="home_provider_login">
      <forward name="success" redirect="true" path="/displayProduct.vn"/>
      <forward name="success1" redirect="true" path="home_provider_authentication"/>
      <forward name="failure" redirect="true" path="home_provider_login"/>
    </action>
    <action path="/logoutProviderAcc" type="ebiz.action.account.provider.Logout">
      <forward name="success" redirect="true" path="/provider.vn"/>
    </action>
    <action path="/registerProvierAcc" name="providerForm" type="ebiz.action.account.provider.Register" scope="session" validate="true" input="home_provider_register">
      <forward name="success" redirect="true" path="home_provider_login"/>
      <forward name="failure" redirect="true" path="home_provider_register"/>
    </action>
    <action path="/uploadProduct" name="foodForm" type="ebiz.action.account.provider.UploadProduct" scope="session" validate="true" input="home_provider_upload">
      <forward name="success" redirect="true" path="home_provider_upload"/>
      <forward name="success1" redirect="true" path="/displayProduct.vn"/>
      <forward name="failure" redirect="true" path="home_provider_upload"/>
      <forward name="login" redirect="false" path="home_provider_login"/>
    </action>
    <action path="/uploadImage" type="ebiz.action.UploadImage" scope="session" validate="true">
      <forward name="success" redirect="true" path="/transferUpImage.vn"/>
      <forward name="success1" redirect="true" path="/transferUpImage1.vn"/>
    </action>
    <action path="/displayProduct" type="ebiz.action.account.provider.DisplayProduct" scope="session" validate="true">
      <forward name="success" redirect="false" path="home_provider"/>
      <forward name="failure" redirect="false" path="home_provider"/>
    </action>
    <action path="/categoryProviderRecord" type="ebiz.action.account.provider.DisplayProductPaging" scope="session" validate="true">
      <forward name="success" redirect="false" path="home_provider"/>
      <forward name="failure" redirect="false" path="home_provider"/>
    </action>
    <action path="/displayDetailProduct" type="ebiz.action.account.provider.DetailProduct">
      <forward name="success" redirect="false" path="home_provider_upload"/>
      <forward name="failure" redirect="false" path="home_provider"/>
    </action>
    <action path="/deleteProduct" type="ebiz.action.account.provider.DeleteProduct">
      <forward name="success" redirect="false" path="/displayProduct.vn"/>
    </action>
    <action path="/resetForm" type="ebiz.action.account.provider.ResetForm">
      <forward name="success" redirect="false" path="/resetForm.vn"/>
    </action>
    <action path="/authenticationProvider" type="ebiz.action.account.provider.Authentication">
      <forward name="success" redirect="false" path="/displayProduct.vn"/>
      <forward name="failure" redirect="false" path="home_provider_authentication"/>
    </action>
    <action path="/displayProviderList" type="ebiz.action.account.provider.ProviderList">
      <forward name="success" redirect="false" path="provider_list"/>
    </action>
    <action path="/searchProvider" type="ebiz.action.account.provider.ProviderSearch">
      <forward name="success" redirect="false" path="provider_list"/>
    </action>
    <action path="/pagingProvider" type="ebiz.action.account.provider.ProviderSearchPaging">
      <forward name="success" redirect="false" path="provider_list"/>
    </action>
    <action path="/displayAddProviderGM" type="ebiz.action.account.provider.GMProviderAddress">
      <forward name="success" redirect="false" path="providerAdd"/>
    </action>
    <action path="/getAddProvider" type="ebiz.action.account.provider.ProviderAdd">
    </action>
    <action path="/getAddProviderList" type="ebiz.action.account.provider.GMProviderAddressList">
      <forward name="success" redirect="false" path="providerAddList"/>
    </action>
    <action path="/cropImage" type="ebiz.action.CropImage">
    </action>
  </action-mappings>
  <controller/>
</struts-config>