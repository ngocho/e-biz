<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent" android:background="@color/transparent"
	android:layout_height="fill_parent">
	<LinearLayout android:id="@+id/linearLayout1" 
		android:layout_height="45dp" android:layout_width="fill_parent"
		android:gravity="center_vertical">
		<TextView android:id="@+id/textView1" android:textColor="@color/black" android:layout_marginLeft="10dp" android:text="TextView" android:layout_weight="1.0"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:layout_width="wrap_content"></TextView>
		<ImageView android:id="@+id/imageView1"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:src="@drawable/icon_arrowslideright"></ImageView>
	</LinearLayout>
</LinearLayout>
