<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/my_background">
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_marginLeft="5dp" android:layout_marginTop="5dp"
		android:layout_marginRight="5dp" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:gravity="center_vertical">
		<ImageView android:src="@drawable/icon_logo"
			android:scaleType="centerInside" android:layout_width="80dp"
			android:layout_height="50dp" android:id="@+id/imageView1"></ImageView>
		<TextView android:id="@+id/textView1" android:textAppearance="?android:attr/textAppearanceSmall"
			android:layout_height="wrap_content" android:layout_weight="1.0"
			android:layout_width="wrap_content"></TextView>
		<Button android:layout_width="wrap_content" android:textColor="@color/white"
			android:textStyle="bold" android:background="@drawable/button_menu" android:id="@+id/menu_login"
			android:layout_height="40dp" android:text="@string/login_login"></Button>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout2"
		android:layout_margin="5dp" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:gravity="clip_horizontal"
		android:layout_gravity="center_horizontal">
		<LinearLayout android:id="@+id/menu_deal"
			android:layout_weight="1.0" android:background="@drawable/button_menu"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:orientation="vertical" android:gravity="center_horizontal">
			<ImageView android:src="@drawable/icon_deals"
				android:layout_marginTop="5dp" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/imageView2"></ImageView>
			<TextView android:id="@+id/textView2" android:textStyle="bold"
				android:textSize="16dp" android:textColor="@color/white"
				android:layout_marginTop="3dp" android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_height="wrap_content" android:text="@string/menu_DEAL"
				android:layout_width="wrap_content"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/menu_best"
			android:clickable="true" android:layout_weight="1.0"
			android:background="@drawable/button_menu" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:orientation="vertical"
			android:gravity="center_horizontal">
			<ImageView android:src="@drawable/icon_listanitem"
				android:layout_marginTop="5dp" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/imageView2"></ImageView>
			<TextView android:id="@+id/textView2" android:textStyle="bold"
				android:textSize="16dp" android:layout_marginTop="3dp"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_height="wrap_content" android:text="@string/menu_best"
				android:textColor="@color/white" android:layout_width="wrap_content"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/menu_browse"
			android:clickable="true" android:layout_weight="1.0"
			android:background="@drawable/button_menu" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:orientation="vertical"
			android:gravity="center_horizontal">
			<ImageView android:src="@drawable/icon_browsecategory"
				android:layout_marginTop="5dp" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/imageView2"></ImageView>
			<TextView android:id="@+id/textView2" android:textStyle="bold"
				android:textSize="16dp" android:layout_marginTop="3dp"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_height="wrap_content" android:text="@string/menu_browser"
				android:textColor="@color/white" android:layout_width="wrap_content"></TextView>
		</LinearLayout>
	</LinearLayout>
	<ScrollView android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/scrollView1">
		<LinearLayout android:id="@+id/linearLayout6"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			<LinearLayout android:id="@+id/linearLayout3"
				android:background="@drawable/button_bg_us"
				android:layout_marginLeft="10dp" android:layout_marginRight="10dp"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout android:id="@+id/menu_save_search"
					android:clickable="true" android:background="@drawable/button_menu_small"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:gravity="center_vertical">
					<ImageView android:layout_width="wrap_content"
						android:layout_margin="2dp" android:layout_height="wrap_content"
						android:id="@+id/imageView3" android:background="@drawable/icon_homescreen_savedsearches_48x48"></ImageView>
					<TextView android:id="@+id/textView3"
						android:layout_marginLeft="5dp" android:layout_weight="1.0"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:textColor="@color/black" android:layout_height="wrap_content"
						android:text="@string/menu_search" android:layout_width="wrap_content"></TextView>
					<ImageView android:src="@drawable/icon_arrowslideright"
						android:layout_marginRight="10dp" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
				<LinearLayout android:layout_height="1dp"
					android:background="@drawable/footer_menu" android:layout_width="fill_parent"
					android:id="@+id/linearLayout5" android:orientation="vertical"></LinearLayout>
				<LinearLayout android:id="@+id/menu_favorite"
					android:clickable="true" android:background="@drawable/button_menu_small"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:gravity="center_vertical">
					<ImageView android:layout_width="wrap_content"
						android:layout_margin="2dp" android:layout_height="wrap_content"
						android:id="@+id/imageView3" android:background="@drawable/icon_homescreen_savedsellers_48x48"></ImageView>
					<TextView android:id="@+id/textView3"
						android:layout_marginLeft="5dp" android:layout_weight="1.0"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:textColor="@color/black" android:layout_height="wrap_content"
						android:text="@string/menu_favorite" android:layout_width="wrap_content"></TextView>
					<ImageView android:src="@drawable/icon_arrowslideright"
						android:layout_marginRight="10dp" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout android:id="@+id/linearLayout3"
				android:layout_marginTop="10dp" android:background="@drawable/button_bg_us"
				android:layout_marginLeft="10dp" android:layout_marginRight="10dp"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout android:id="@+id/menu_reminder"
					android:clickable="true" android:background="@drawable/button_menu_small"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:gravity="center_vertical">
					<ImageView android:layout_width="wrap_content"
						android:layout_margin="2dp" android:layout_height="wrap_content"
						android:id="@+id/imageView3" android:background="@drawable/icon_homescreen_reminders_48x48"></ImageView>
					<TextView android:id="@+id/textView3"
						android:layout_marginLeft="5dp" android:layout_weight="1.0"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:textColor="@color/black" android:layout_height="wrap_content"
						android:text="@string/menu_reminder" android:layout_width="wrap_content"></TextView>
					<ImageView android:src="@drawable/icon_arrowslideright"
						android:layout_marginRight="10dp" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
				<LinearLayout android:layout_height="1dp"
					android:background="@drawable/footer_menu" android:layout_width="fill_parent"
					android:id="@+id/linearLayout5" android:orientation="vertical"></LinearLayout>
				<LinearLayout android:id="@+id/linearLayout4"
					android:clickable="true" android:background="@drawable/button_menu_small"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:gravity="center_vertical">
					<ImageView android:layout_width="wrap_content"
						android:layout_margin="2dp" android:layout_height="wrap_content"
						android:id="@+id/imageView3" android:background="@drawable/icon_homescreen_messages_48x48"></ImageView>
					<TextView android:id="@+id/textView3"
						android:layout_marginLeft="5dp" android:layout_weight="1.0"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:textColor="@color/black" android:layout_height="wrap_content"
						android:text="@string/menu_message" android:layout_width="wrap_content"></TextView>
					<ImageView android:src="@drawable/icon_arrowslideright"
						android:layout_marginRight="10dp" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout android:id="@+id/linearLayout3"
				android:layout_marginTop="10dp" android:background="@drawable/button_bg_us"
				android:layout_marginLeft="10dp" android:layout_marginRight="10dp"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout android:id="@+id/menu_my_account"
					android:clickable="true" android:background="@drawable/button_menu_small"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:gravity="center_vertical">
					<ImageView android:layout_width="wrap_content"
						android:layout_margin="2dp" android:layout_height="wrap_content"
						android:id="@+id/imageView3" android:background="@drawable/icon_homescreen_myebay_48x48"></ImageView>
					<TextView android:id="@+id/textView3"
						android:layout_marginLeft="5dp" android:layout_weight="1.0"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:textColor="@color/black" android:layout_height="wrap_content"
						android:text="@string/menu_account" android:layout_width="wrap_content"></TextView>
					<ImageView android:src="@drawable/icon_arrowslideright"
						android:layout_marginRight="10dp" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
				<LinearLayout android:layout_height="1dp"
					android:background="@drawable/footer_menu" android:layout_width="fill_parent"
					android:id="@+id/linearLayout5" android:orientation="vertical"></LinearLayout>
				<LinearLayout android:id="@+id/menu_payment"
					android:clickable="true" android:background="@drawable/button_menu_small"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:gravity="center_vertical">
					<ImageView android:layout_width="wrap_content"
						android:layout_margin="2dp" android:layout_height="wrap_content"
						android:id="@+id/imageView3" android:background="@drawable/icon_homescreen_deals_48x48"></ImageView>
					<TextView android:id="@+id/textView3"
						android:layout_marginLeft="5dp" android:layout_weight="1.0"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:textColor="@color/black" android:layout_height="wrap_content"
						android:text="@string/menu_payment" android:layout_width="wrap_content"></TextView>
					<ImageView android:src="@drawable/icon_arrowslideright"
						android:layout_marginRight="10dp" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout android:id="@+id/linearLayout3"
				android:layout_marginTop="10dp" android:background="@drawable/button_bg_us"
				android:layout_marginLeft="10dp" android:layout_marginRight="10dp"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout android:id="@+id/menu_about"
					android:clickable="true" android:background="@drawable/button_menu_small"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:gravity="center_vertical">
					<ImageView android:layout_width="wrap_content"
						android:layout_margin="2dp" android:layout_height="wrap_content"
						android:id="@+id/imageView3" android:background="@drawable/icon_homescreen_settings_48x48"></ImageView>
					<TextView android:id="@+id/textView3"
						android:layout_marginLeft="5dp" android:layout_weight="1.0"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:textColor="@color/black" android:layout_height="wrap_content"
						android:text="@string/menu_setting" android:layout_width="wrap_content"></TextView>
					<ImageView android:src="@drawable/icon_arrowslideright"
						android:layout_marginRight="10dp" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
