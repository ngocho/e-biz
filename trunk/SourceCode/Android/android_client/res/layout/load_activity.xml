<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:background="@drawable/backgroundmenu" android:layout_height="fill_parent"
	android:gravity="center_horizontal">
	<ImageView android:layout_height="200dp"
		android:layout_marginTop="50dp" android:background="@drawable/logo2"
		android:id="@+id/imageView1" android:layout_width="200dp"></ImageView>
	<TextView android:layout_marginLeft="10dp" android:layout_height="wrap_content" android:layout_marginTop="35dp" android:layout_gravity="left"
		android:textStyle="bold" android:textColor="@color/white" android:layout_marginBottom="5dp"
		android:textSize="16dp" android:text="@string/about_doan"
		android:layout_width="wrap_content" android:id="@+id/textView1"></TextView>
	<TextView android:layout_height="wrap_content"
		android:textStyle="bold" android:textColor="@color/white"
		android:textSize="18dp" android:text="@string/about_name"
		android:layout_width="wrap_content" android:id="@+id/textView1" android:gravity="center_horizontal"></TextView>
</LinearLayout>
