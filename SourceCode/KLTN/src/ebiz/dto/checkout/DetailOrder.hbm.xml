<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ebiz.dto.checkout.DetailOrder" table="DetailOrder">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="idProduct" type="java.lang.Long">
            <column name="idProduct" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
        <property name="number" type="java.lang.Integer">
            <column name="number" />
        </property>
        <property name="subPrice" type="java.lang.Long">
            <column name="subPrice" />
        </property>
        <property name="orderId" type="java.lang.Long">
            <column name="orderId" />
        </property>
    </class>
</hibernate-mapping>
