<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ebiz.dto.food.FoodStatus" table="tblFoodStatus">
        <id name="foodStatusId" type="java.lang.String">
            <column name="foodStatusId" />
            <generator class="assigned" />
        </id>
        <property name="foodStatusName" type="java.lang.String">
            <column name="foodStatusName" />
        </property>
    </class>
</hibernate-mapping>
