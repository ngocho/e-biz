<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical">
	<LinearLayout android:layout_height="fill_parent"
		android:orientation="vertical" android:layout_margin="10dp"
		android:id="@+id/linearLayout1" android:layout_width="fill_parent">
		<TextView android:text="@string/introduction"
			android:textColor="@color/black" android:id="@+id/sdsd"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<TextView android:layout_marginLeft="10dp" android:text="TextView"
			android:textColor="@color/black" android:id="@+id/tab_detail_content_info"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<TextView android:text="@string/cooking"
			android:layout_marginTop="5dp" android:textColor="@color/black"
			android:id="@+id/sdddsd" android:layout_width="wrap_content"
			android:layout_height="wrap_content"></TextView>
		<TextView android:text="TextView" android:textColor="@color/black"
			android:layout_marginLeft="10dp" android:id="@+id/tab_detail_content_use"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	</LinearLayout>
</LinearLayout>
