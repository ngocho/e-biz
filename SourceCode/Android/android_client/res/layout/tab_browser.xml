<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:background="@drawable/my_background" android:layout_height="fill_parent">
	<LinearLayout android:id="@+id/linearLayout1"
		android:background="@drawable/theme_activity_title_bar"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_gravity="center_vertical" android:gravity="center_vertical">
		<Button android:id="@+id/tab_browser_back" android:background="@drawable/btn_back"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:gravity="left" android:layout_gravity="left"></Button>
		<LinearLayout android:layout_width="fill_parent"
			android:id="@+id/linearLayout4" android:layout_height="fill_parent"
			android:gravity="center_vertical|center_horizontal">
			<TextView android:textStyle="bold" android:gravity="center_horizontal"
				android:id="@+id/textView1" android:textColor="@color/white"
				android:layout_marginRight="40dp" android:layout_height="wrap_content"
				android:textSize="18dp" android:layout_width="wrap_content"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:text="@string/menu_browser"></TextView>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout2"
		android:layout_marginLeft="10dp" android:layout_marginRight="5dp"
		android:layout_marginTop="2dp" android:layout_marginBottom="2dp"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:gravity="center_vertical">
		<EditText android:background="@drawable/search_bg_bv"
			android:paddingTop="1dp" android:layout_weight="1.0"
			android:id="@+id/tab_browser_edit" android:layout_width="fill_parent"
			android:layout_height="wrap_content"></EditText>
		<Button android:id="@+id/tab_browser_ok" android:background="@drawable/search_button_none"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
		<Button android:text="@string/tab_browser_save" android:id="@+id/tab_browser_savesearch"
			android:paddingRight="10dp" android:paddingLeft="10dp"
			android:textColor="@color/white" android:background="@drawable/button_menu"
			android:layout_width="wrap_content" android:layout_height="35dp"></Button>
	</LinearLayout>
	<ListView android:id="@+id/tab_browser_list"
		android:listSelector="@color/transparent" android:layout_margin="5dp"
		android:layout_height="wrap_content" android:cacheColorHint="#00000000"
		android:layout_weight="1.0" android:layout_width="fill_parent"></ListView>
	<LinearLayout android:id="@+id/linearLayout2"
		android:background="@drawable/footer_menu" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:gravity="clip_horizontal"
		android:layout_gravity="center_horizontal">
		<LinearLayout android:id="@+id/tab_browser_home"
			android:layout_weight="1.0" android:background="@drawable/button_menu"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:orientation="vertical" android:gravity="center_horizontal">
			<ImageView android:src="@drawable/icon_menu_home"
				android:layout_marginTop="1dp" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/imageView2"></ImageView>
			<TextView android:id="@+id/textView2" android:textSize="12dp"
				android:textColor="@color/white" android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_height="wrap_content" android:text="@string/menu_home"
				android:layout_width="wrap_content"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/tab_browser_deal"
			android:layout_weight="1.0" android:background="@drawable/button_menu"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:orientation="vertical" android:gravity="center_horizontal">
			<ImageView android:src="@drawable/icon_deals"
				android:layout_marginTop="1dp" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/imageView2"></ImageView>
			<TextView android:id="@+id/textView2" android:textSize="12dp"
				android:textColor="@color/white" android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_height="wrap_content" android:text="@string/menu_DEAL"
				android:layout_width="wrap_content"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/tab_browser_best"
			android:clickable="true" android:layout_weight="1.0"
			android:background="@drawable/button_menu" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:orientation="vertical"
			android:gravity="center_horizontal">
			<ImageView android:src="@drawable/icon_listanitem"
				android:layout_marginTop="1dp" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/imageView2"></ImageView>
			<TextView android:id="@+id/textView2" android:textSize="12dp"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_height="wrap_content" android:text="@string/menu_best"
				android:textColor="@color/white" android:layout_width="wrap_content"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/tab_browser_browse"
			android:clickable="true" android:layout_weight="1.0"
			android:background="@drawable/button_menu" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:orientation="vertical"
			android:gravity="center_horizontal">
			<ImageView android:src="@drawable/icon_browsecategory"
				android:layout_marginTop="1dp" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/imageView2"></ImageView>
			<TextView android:id="@+id/textView2" android:textSize="12dp"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_height="wrap_content" android:text="@string/menu_browser"
				android:textColor="@color/white" android:layout_width="wrap_content"></TextView>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
